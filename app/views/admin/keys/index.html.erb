<h1 class="mainHeading">Current Keys</h1>
    <hr class="faded" />

        <%= link_to "Back", :back, class: :formLink %>

<table class="table">
        <tr>
            <th class="tableHeading">Key ID</th>
            <th class="tableHeading">Key Name</th>
            <th class="tableHeading">User ID</th>
            <th class="tableHeading">Email</th>
        </tr>
    <% @Keys.each do | key | %>
        <tr>
            <td class="tableValue"><%= key.id %></td>
            <td class="tableValue"><%= key.name %></td>
            <td class="tableValue"><%= key.user_id %></td>
            <td class="tableValue"><%= key.user.email %></td>
        </tr>
    <% end %>
</table>
